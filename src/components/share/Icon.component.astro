// Función para obtener el icono con los atributos aplicados
const getIconWithAttributes = (
  icon: string,
  className: string = '',
  width: number = defaultWidth,
  height: number = defaultHeight
): string => {
  return icon.replace(
    '<svg ',
    `<svg class="${className}" width="${width}" height="${height}" `
  )
}

// Obtener las propiedades pasadas al componente
const {
  name,
  height = defaultHeight,
  width = defaultWidth,
  className = ''
} = Astro.props as Props

// Obtener el icono en bruto
const rawIcon = ICONS[name] || ''
// Aplicar los atributos al icono
const iconWithAttributes = getIconWithAttributes(
  rawIcon,
  className,
  width,
  height
);
